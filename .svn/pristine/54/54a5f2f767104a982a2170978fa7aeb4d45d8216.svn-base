package com.tmtc.po;

import org.springframework.format.annotation.DateTimeFormat;

import java.util.Date;

public class UserDetail {
	private String id;

	private String link_num;

	@DateTimeFormat(pattern = "yyyy-MM-dd HH:mm:ss")
	private Date register_time;

	private Integer register_source;

	private String username;

	private String nick_name;
	
	private String real_name;

	private String qr_code;

	private String qr_code_secret;
	
	private Integer sex;
	
	private String id_code;
	
	@DateTimeFormat(pattern = "yyyy-MM-dd")
	private Date birthday;
	
	private Integer user_type;

	private String portrait;

	private String background;
	
	private String email;
	
	private String address;

	private String card_num;

	private Integer balance;

	private Integer exp_score;

	private String remark;

	private String invitation_code;
	
	@DateTimeFormat(pattern = "yyyy-MM-dd HH:mm:ss")
	private Date last_order_time;

	private Integer is_check;

	public UserDetail() {
	}

	public UserDetail(Integer user_type, String id, String link_num, Date register_time, Integer register_source,
			String username, String nick_name, String real_name, String qr_code, String qr_code_secret, Integer sex,
			String id_code, Date birthday, String portrait, String background, String email, String address,
			String card_num, Integer exp_score, String remark, String invitation_code, Date last_order_time,
			Integer is_check) {
		this.user_type = user_type;
		this.id = id;
		this.link_num = link_num;
		this.register_time = register_time;
		this.register_source = register_source;
		this.username = username;
		this.nick_name = nick_name;
		this.real_name = real_name;
		this.qr_code = qr_code;
		this.qr_code_secret = qr_code_secret;
		this.sex = sex;
		this.id_code = id_code;
		this.birthday = birthday;
		this.portrait = portrait;
		this.background = background;
		this.email = email;
		this.address = address;
		this.card_num = card_num;
		this.exp_score = exp_score;
		this.remark = remark;
		this.invitation_code = invitation_code;
		this.last_order_time = last_order_time;
		this.is_check = is_check;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getLink_num() {
		return link_num;
	}

	public void setLink_num(String link_num) {
		this.link_num = link_num;
	}

	public Date getRegister_time() {
		return register_time;
	}

	public void setRegister_time(Date register_time) {
		this.register_time = register_time;
	}

	public Integer getRegister_source() {
		return register_source;
	}

	public void setRegister_source(Integer register_source) {
		this.register_source = register_source;
	}

	public String getUsername() {
		return username;
	}

	public void setUsername(String username) {
		this.username = username;
	}

	public String getNick_name() {
		return nick_name;
	}

	public void setNick_name(String nick_name) {
		this.nick_name = nick_name;
	}

	public String getReal_name() {
		return real_name;
	}

	public void setReal_name(String real_name) {
		this.real_name = real_name;
	}

	public String getQr_code() {
		return qr_code;
	}

	public void setQr_code(String qr_code) {
		this.qr_code = qr_code;
	}

	public String getQr_code_secret() {
		return qr_code_secret;
	}

	public void setQr_code_secret(String qr_code_secret) {
		this.qr_code_secret = qr_code_secret;
	}

	public Integer getSex() {
		return sex;
	}

	public void setSex(Integer sex) {
		this.sex = sex;
	}

	public String getId_code() {
		return id_code;
	}

	public void setId_code(String id_code) {
		this.id_code = id_code;
	}

	public Date getBirthday() {
		return birthday;
	}

	public void setBirthday(Date birthday) {
		this.birthday = birthday;
	}

	public Integer getUser_type() {
		return user_type;
	}

	public void setUser_type(Integer user_type) {
		this.user_type = user_type;
	}

	public String getPortrait() {
		return portrait;
	}

	public void setPortrait(String portrait) {
		this.portrait = portrait;
	}

	public String getBackground() {
		return background;
	}

	public void setBackground(String background) {
		this.background = background;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getAddress() {
		return address;
	}

	public void setAddress(String address) {
		this.address = address;
	}

	public String getCard_num() {
		return card_num;
	}

	public void setCard_num(String card_num) {
		this.card_num = card_num;
	}

	public Integer getBalance() {
		return balance;
	}

	public void setBalance(Integer balance) {
		this.balance = balance;
	}

	public Integer getExp_score() {
		return exp_score;
	}

	public void setExp_score(Integer exp_score) {
		this.exp_score = exp_score;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getInvitation_code() {
		return invitation_code;
	}

	public void setInvitation_code(String invitation_code) {
		this.invitation_code = invitation_code;
	}

	public Date getLast_order_time() {
		return last_order_time;
	}

	public void setLast_order_time(Date last_order_time) {
		this.last_order_time = last_order_time;
	}

	public Integer getIs_check() {
		return is_check;
	}

	public void setIs_check(Integer is_check) {
		this.is_check = is_check;
	}

	@Override
	public String toString() {
		return "UserDetail [id=" + id + ", link_num=" + link_num + ", register_time=" + register_time
				+ ", register_source=" + register_source + ", username=" + username + ", nick_name=" + nick_name
				+ ", real_name=" + real_name + ", qr_code=" + qr_code + ", qr_code_secret=" + qr_code_secret + ", sex="
				+ sex + ", id_code=" + id_code + ", birthday=" + birthday + ", user_type=" + user_type + ", portrait="
				+ portrait + ", background=" + background + ", email=" + email + ", address=" + address + ", card_num="
				+ card_num + ", balance=" + balance + ", exp_score=" + exp_score + ", remark=" + remark
				+ ", invitation_code=" + invitation_code + ", last_order_time=" + last_order_time + ", is_check="
				+ is_check + "]";
	}
}