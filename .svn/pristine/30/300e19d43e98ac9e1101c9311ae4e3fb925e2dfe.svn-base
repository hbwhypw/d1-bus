package com.tmtc.po;

import java.util.Date;
import java.util.List;

import org.springframework.format.annotation.DateTimeFormat;

public class Line {
	private String id;

	private String line_name;

	private String start_id;

	private String end_id;

	private Integer price;

	private Double km;

	private Double on_time_rate;

	private Integer city_id;

	private String company_id;

	private Integer line_category;

	private Integer line_type;

	private Integer user_type;

	@DateTimeFormat(pattern = "yyyy-MM-dd HH:mm:ss")
	private Date start_time;

	@DateTimeFormat(pattern = "yyyy-MM-dd HH:mm:ss")
	private Date end_time;

	private Integer sort;

	private String remark;

	private Date create_time;

	private Integer is_check;

	private String line_path;

	private List<LineTime> lineTimeList;

	private List<LineStation> lineStationList;

	private LineStation startLineStation;

	private LineStation endLineStation;

	public Line() {
		super();
	}

	public Line(String id, String line_name, String start_id, String end_id, Integer price, Double km,
			Double on_time_rate, Integer city_id, String company_id, Integer line_category, Integer line_type,
			Integer user_type, Date start_time, Date end_time, Integer sort, String remark, Date create_time,
			Integer is_check, String line_path) {
		super();
		this.id = id;
		this.line_name = line_name;
		this.start_id = start_id;
		this.end_id = end_id;
		this.price = price;
		this.km = km;
		this.on_time_rate = on_time_rate;
		this.city_id = city_id;
		this.company_id = company_id;
		this.line_category = line_category;
		this.line_type = line_type;
		this.user_type = user_type;
		this.start_time = start_time;
		this.end_time = end_time;
		this.sort = sort;
		this.remark = remark;
		this.create_time = create_time;
		this.is_check = is_check;
		this.line_path = line_path;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getLine_name() {
		return line_name;
	}

	public void setLine_name(String line_name) {
		this.line_name = line_name;
	}

	public String getStart_id() {
		return start_id;
	}

	public void setStart_id(String start_id) {
		this.start_id = start_id;
	}

	public String getEnd_id() {
		return end_id;
	}

	public void setEnd_id(String end_id) {
		this.end_id = end_id;
	}

	public Integer getPrice() {
		return price;
	}

	public void setPrice(Integer price) {
		this.price = price;
	}

	public Double getKm() {
		return km;
	}

	public void setKm(Double km) {
		this.km = km;
	}

	public Double getOn_time_rate() {
		return on_time_rate;
	}

	public void setOn_time_rate(Double on_time_rate) {
		this.on_time_rate = on_time_rate;
	}

	public Integer getCity_id() {
		return city_id;
	}

	public void setCity_id(Integer city_id) {
		this.city_id = city_id;
	}

	public String getCompany_id() {
		return company_id;
	}

	public void setCompany_id(String company_id) {
		this.company_id = company_id;
	}

	public Integer getLine_category() {
		return line_category;
	}

	public void setLine_category(Integer line_category) {
		this.line_category = line_category;
	}

	public Integer getLine_type() {
		return line_type;
	}

	public void setLine_type(Integer line_type) {
		this.line_type = line_type;
	}

	public Integer getUser_type() {
		return user_type;
	}

	public void setUser_type(Integer user_type) {
		this.user_type = user_type;
	}

	public Date getStart_time() {
		return start_time;
	}

	public void setStart_time(Date start_time) {
		this.start_time = start_time;
	}

	public Date getEnd_time() {
		return end_time;
	}

	public void setEnd_time(Date end_time) {
		this.end_time = end_time;
	}

	public Integer getSort() {
		return sort;
	}

	public void setSort(Integer sort) {
		this.sort = sort;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public Date getCreate_time() {
		return create_time;
	}

	public void setCreate_time(Date create_time) {
		this.create_time = create_time;
	}

	public Integer getIs_check() {
		return is_check;
	}

	public void setIs_check(Integer is_check) {
		this.is_check = is_check;
	}

	public String getLine_path() {
		return line_path;
	}

	public void setLine_path(String line_path) {
		this.line_path = line_path;
	}

	public List<LineTime> getLineTimeList() {
		return lineTimeList;
	}

	public void setLineTimeList(List<LineTime> lineTimeList) {
		this.lineTimeList = lineTimeList;
	}

	public List<LineStation> getLineStationList() {
		return lineStationList;
	}

	public void setLineStationList(List<LineStation> lineStationList) {
		this.lineStationList = lineStationList;
	}

	public LineStation getStartLineStation() {
		return startLineStation;
	}

	public void setStartLineStation(LineStation startLineStation) {
		this.startLineStation = startLineStation;
	}

	public LineStation getEndLineStation() {
		return endLineStation;
	}

	public void setEndLineStation(LineStation endLineStation) {
		this.endLineStation = endLineStation;
	}

	@Override
	public String toString() {
		return "Line [id=" + id + ", line_name=" + line_name + ", start_id=" + start_id + ", end_id=" + end_id
				+ ", price=" + price + ", km=" + km + ", on_time_rate=" + on_time_rate + ", city_id=" + city_id
				+ ", company_id=" + company_id + ", line_category=" + line_category + ", line_type=" + line_type
				+ ", user_type=" + user_type + ", start_time=" + start_time + ", end_time=" + end_time + ", sort="
				+ sort + ", remark=" + remark + ", create_time=" + create_time + ", is_check=" + is_check
				+ ", line_path=" + line_path + "]";
	}
}